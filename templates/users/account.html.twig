{% extends 'base.html.twig' %}
{% block title %}
	- mon compte
{% endblock %}
{% block contentPage %}
	<p>
		Email:
		{{ app.user.email }}
	</p>
	<p>
		Nom:
		{{ app.user.lastname }}
	</p>
	<p>
		Prénom:
		{{ app.user.firstname }}
	</p>
	<p>
		adresse:
		{{ app.user.adress }}
	</p>
	<p>
		Code postal:
		{{ app.user.zipcode }}
	</p>
	<p>
		ville:
		{{ app.user.city }}
	</p>
	<p>
		compte crée le :

		{{ app.user.CreatedAt|date("d/m/Y") }}
	</p>
	<a href="{{ path('app_logout') }}">
		Me déconnecté
	</a>
{% endblock %}
